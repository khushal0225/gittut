#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */
void swap(int *a,int *b)
{
	int temp;
	temp=*a;
	*a=*b;
	*b=temp;
	printf("\n value of a is : %d",*a);
	printf("\n value of b is : %d",*b);
	
}
int main() {
	int n1,n2;
	n1=10;
	n2=20;
	printf("\n value of n1 is : %d",n1);
	printf("\n value of n2 is : %d",n2);
	swap(&n1,&n2);
	printf("\n value of n1 after swap function : %d",n1);
	printf("\n value of n2 after swap function : %d",n2);

	
	return 0;
}